{
  "name": "simplicite",
  "version": "1.1.1",
  "description": "Simplicite(R) platform Javascript API (for node.js and browser)",
  "repository": {
    "type": "git",
    "url": "git://github.com/simplicitesoftware/nodejs-api.git"
  },
  "keywords": [
    "simplicite",
    "simplicitesoftware",
    "apaas",
    "lowcode"
  ],
  "dependencies": {
